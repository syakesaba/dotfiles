[alias]
show = log --stat --color=always -vpr
last = log -l1 -p
df = diff
qstatus = status --rev -2:-1
blame = blame --number --user --date -q
glog = log --graph
today = !hg log --template '{rev} {desc|firstline|strip}\n' -r "sort(user('$(hg showconfig ui.username | sed 's/.*<\(.*\)>/\1/')') and date(-1), -date)"

